#:import MapView kivy_garden.mapview.MapView

<Gps>:

    id: map
    lat: 40.62961
    lon: -8.65810
    zoom: 20
    double_tap_zoom: True
    on_zoom:
        self.zoom = 5 if self.zoom <5 else self.zoom

    MapMarkerPopup:
        id: map_marker
        #source:"atlas.png"
        lat:40.62961
        lon:-8.65810
